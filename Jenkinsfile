    agent any

    stages {
        stage('Code') {
            steps {
                echo 'This is code stage'
            }
        }
        stage('Build') {
            steps {
                echo 'This is Build stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is deploy stage'
            }
        }
    }
}

